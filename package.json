{
  "name": "react-emotion-styles-guide",
  "version": "0.3.3",
  "description": "",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MauroTaliente/react-emotion-styles-guide.git"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,dev,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "dev": "parcel dev/index.html",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "styles",
    "emotion",
    "jss"
  ],
  "author": "Mauro Taliente (MauroTaliente)",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/facepaint": "^1.2.2",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/ramda": "^0.28.20",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.3.1",
    "parcel": "^2.8.2",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "facepaint": "^1.2.1",
    "ramda": "^0.28.0",
    "runtypes": "^6.6.0"
  },
  "peerDependencies": {
    "react": ">=16"
  }
}
